          ],
          "properties": {
            "display_unit": "%",
            "max_limit": "100"
          }
        },
        {
          "widget_name": "NodeManager GC Time",
          "description": "",
          "widget_type": "HEATMAP",
          "is_visible": false,
          "metrics": [
            {
              "name": "Hadoop:service=NodeManager,name=JvmMetrics.GcTimeMillis",
              "metric_path": "metrics/jvm/gcTimeMillis",
